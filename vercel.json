{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2",
            "maxDuration": 60
        },
        "api/warmup.php": {
            "runtime": "vercel-php@0.7.2",
            "maxDuration": 10
        },
        "api/debug.php": {
            "runtime": "vercel-php@0.7.2",
            "maxDuration": 15
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/resources/(.*)",
            "dest": "/public/resources/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/images/ratings/(.*)",
            "dest": "/public/images/ratings/$1",
            "headers": {
                "Cache-Control": "public, max-age=86400"
            }
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1",
            "headers": {
                "Cache-Control": "public, max-age=86400"
            }
        },
        {
            "src": "/theme/(.*)",
            "dest": "/public/theme/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/favicon/(.*)",
            "dest": "/public/favicon/$1",
            "headers": {
                "Cache-Control": "public, max-age=86400"
            }
        },
        {
            "src": "/plugins/(.*)",
            "dest": "/public/plugins/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/logo/(.*)",
            "dest": "/public/logo/$1",
            "headers": {
                "Cache-Control": "public, max-age=86400"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "file",
        "SESSION_DRIVER": "cookie",
        "LOG_CHANNEL": "stderr",
        "FILESYSTEM_DISK": "local"
    },
    "outputDirectory": "public"
}
